#include <stdio.h>
#include <math.h>
#include <stdbool.h>

bool perfect_square(double);

int main() {
    // x + 100 = n^2
    // x + 100 + 168 = m^2
    for (int i = 2; i <= 85; i++){
        double n = sqrt(i+100);
        double m = sqrt(i+100+168); 
        if (perfect_square(n) == true && perfect_square(m) == true){
            printf ("%If",i);
        }
    }
    return 0;
}

bool perfect_square(double n){
    for (double i = 1; i <= n; i++){
       if (n*n == i){
           return true;
       }
    return false;
    }
}
